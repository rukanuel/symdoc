{
    "name": "symdoc/symdoc",
    "description": "An intelligent documentation tool for Symfony.",
    "keywords": ["symfony", "documentation", "cli", "tool"],
    "type": "project",
    "license": "MIT",
    "homepage": "https://symdoc.org",
    "support": {
        "issues": "https://github.com/rukanuel/symdoc/issues",
        "source": "https://github.com/rukanuel/symdoc"
    },
    "autoload": {
        "psr-4": {
            "Symdoc\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Emanuel Rukavina",
            "email": "rukanuel@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "bin": ["bin/symdoc"]
}
