{
    "name": "symdoc/symdoc",
    "description": "An intelligent documentation generator for Symfony.",
    "keywords": [
        "symfony",
        "symdoc",
        "documentation",
        "cli",
        "tool"
    ],
    "type": "library",
    "license": "LGPL-2.1-only",
    "homepage": "https://symdoc.org",
    "support": {
        "issues": "https://github.com/rukanuel/symdoc/issues",
        "source": "https://github.com/rukanuel/symdoc"
    },
    "autoload": {
        "psr-4": {
            "Symdoc\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Emanuel Rukavina",
            "email": "rukanuel@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "bin": [
        "bin/symdoc"
    ]
}